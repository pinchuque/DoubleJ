DELETE FROM OrderDetail WHERE ID IN (
SELECT DISTINCT OrderDetail.ID FROM Customer
	LEFT JOIN CustomerLocation ON CustomerLocation.CustomerId = Customer.Id
	LEFT JOIN [Order] ON [Order].CustomerId = Customer.Id
	LEFT JOIN OrderDetail ON OrderDetail.OrderId = [Order].Id OR OrderDetail.CustomerLocationId = CustomerLocation.Id
	WHERE Customer.Name like '%James Viola%'
)

DELETE FROM ColdWeight WHERE ID IN (
SELECT DISTINCT ColdWeight.ID FROM Customer
	LEFT JOIN [Order] ON [Order].CustomerId = Customer.Id
	LEFT JOIN ColdWeight ON ColdWeight.OrderId = [Order].Id
	WHERE Customer.Name like '%James Viola%'
)

