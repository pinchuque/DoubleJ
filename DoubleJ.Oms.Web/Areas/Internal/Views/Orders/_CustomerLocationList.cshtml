@model List<CheckBoxItem>

@if (Model.Count > 0)
{
    <ul class="checkboxList">
        @for (int i = 0; i < Model.Count; i++)
        {
            <li>@Html.EditorFor(p => Model[i], "EditorTemplates/CheckBoxItem", string.Format("CustomerLocationList[{0}]", i))</li>
        }
    </ul>
}
else
{
    <i>Please select an owner.</i>
}
